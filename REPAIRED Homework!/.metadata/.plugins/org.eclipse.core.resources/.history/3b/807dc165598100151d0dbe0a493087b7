import java.util.Scanner;

public class first {

	public static void main(String[] args) {
		Scanner test = new Scanner(System.in, "UTF-8");
		
		System.out.print("Въведи първо число: ");
		int sm = test.nextInt();
		
		System.out.println("Въведи второ число: ");
		int sm1 = test.nextInt();
		System.out.println(sm + sm1);
		test.close();

	}

}
