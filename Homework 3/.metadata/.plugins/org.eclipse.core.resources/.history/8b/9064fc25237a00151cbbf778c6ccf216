
public class sm 
{

	public static void main(String[] args)
	{
		int a = 5;
		int b = 10;
		int c = (a*b)/2;
		
		System.out.print("a = ");
		System.out.println(a);
		
		System.out.print("b = ");
		System.out.println(b);
		
		System.out.println("((a*b)/2)");
		
		System.out.print("c = ");
		System.out.print(c);
		

	}

}
