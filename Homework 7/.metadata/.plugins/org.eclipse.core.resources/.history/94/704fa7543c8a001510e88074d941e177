
public class sm1 {

	public static void main(String[] args) {
		int[][] a = new int [][] {
			{1, 3, 5, 7},
			{50, 22, 55, 80},
			{10, 33, 100,
				90, 100},
			{20,5}
		};
		for (int i = 0; i < a.length; i++) {
			for (int j = 0; j < a.length; j++) {
				if(a[i][j] % 2 == 0){
					System.out.print(a[i][j] + " ");
				}
			}
		}
	}

}
