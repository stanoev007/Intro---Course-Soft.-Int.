import java.util.Scanner;

public class sm3 {

	public static void main(String[] args) {
		Scanner a = new Scanner(System.in);
		
		System.out.println("Въведи n: ");
		int n = a.nextInt();

		int min = 30;
		int max = -350;

		for (int i = 1; i < n; i++) {
			if (n > max) {
				n = max;
			}
			if (n < min) {
				n = min;
			}
		}

		a.close();
	}

}
